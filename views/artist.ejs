<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>Music Player</title>
    <link rel="stylesheet" type="text/css" href="/css/stylesheet.css">
  </head>
  <body>
    <div class="container">
      <%- include('partials/navbar') %>
    </div>
    <p><%= name %></p>
    <p><%= genre %></p>
    <% albums.forEach((album) => { %>
      <a href="/albums/<%= album.id %>" class="album-info-link">
        <p><%= album.title %></p>
      </a>
      <p><%= album.year %></p>
    <% }) %>
  </body>
</html>
